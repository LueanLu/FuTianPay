  body
    //- #app
      //- Navigation
      include navi.pug
      //- Fixed
      include fixed.pug
      //- Main Content
      main#top.main.min-vh-100
        //- Preloader
        include preloader.pug
        //- <section> IndexBanner
        include indexBanner.pug
        //- Link Tag
        #target-down
        //- Advantage
        include advantage.pug
        //- <section> IndexServices
        #digital-features
          .container-fluid
            #carouselExampleIndicators.carousel.slide.IndexServiceItems(data-ride="carousel")
              .row.justify-content-center.align-items-center.text-center
                .col-lg-7.TitleFormB
                  h2.Title {{ $t('IndexServiceItems.Title'/*SERVICES*/) }}
                  .d-flex.flex-wrap.justify-content-center.align-items-center
                    .IndexServiceMainTitle {{ $t('IndexServiceItems.ServiceCounty'/*服務國家*/) }}
                    ol.carousel-indicators.d-flex.flex-wrap
                      li(data-target="#carouselExampleIndicators" data-slide-to="0" class="active")
                        span {{ $t('IndexServiceItems.Taiwan'/*台灣*/) }}
                      li(data-target="#carouselExampleIndicators" data-slide-to="1")
                        span {{ $t('IndexServiceItems.China'/*中國*/) }}
                      li(data-target="#carouselExampleIndicators" data-slide-to="2")
                        span {{ $t('IndexServiceItems.Thailand'/*泰國*/) }}
                      li(data-target="#carouselExampleIndicators" data-slide-to="3")
                        span {{ $t('IndexServiceItems.Vietnam'/*越南*/) }}
                      li(data-target="#carouselExampleIndicators" data-slide-to="4")
                        span {{ $t('IndexServiceItems.Usdt'/*USDT*/) }}
              .row
                .carousel-inner(data-zanim-timeline='{"delay":0.1}' data-zanim-trigger="scroll")
                  //- 台灣
                  .carousel-item(class="active")
                    include indexServicesTw
                  //- 中國
                  .carousel-item
                    include indexServicesCh
                  //- 泰國
                  .carousel-item
                    include indexServicesTh
                  //- 越南
                  .carousel-item
                    include indexServicesVi
                  //- USDT
                  .carousel-item
                    include indexServicesUSDT
              //- Prev & Next Button
              a.carousel-control-prev.d-lg-block.d-none(href="#carouselExampleIndicators" role="button" data-slide="prev")
                span.carousel-control-prev-icon(aria-hidden="true")
                span.sr-only Previous
              a.carousel-control-next.d-lg-block.d-none(href="#carouselExampleIndicators" role="button" data-slide="next")
                span.carousel-control-next-icon(aria-hidden="true")
                span.sr-only Next
        //- <section> Advertising
        include advertising.pug
        //- <section> Contact
        include indexContact.pug
        //- <section> Company
        include company.pug
      //- Footer Copyright
      include footer.pug

    #app
      main#top.main.min-vh-100
        //- Preloader

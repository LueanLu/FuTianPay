        //- End of Preloader
        //- <section> begin
        section#page-404.py-0
          .container-fluid
            .row
              .col-lg-6.px-0.order-lg-2
                .sticky-top.vh-lg-100.py-9
                  .bg-holder(style="background-image:url(assets/img/error/404.jpg);" data-zanim-trigger="scroll" data-zanim-lg='{"animation":"zoom-out-slide-left","delay":0.4}')
                  // /.bg-holder
              .col-lg-6.bg-white.py-6
                .row.h-100.align-items-center.justify-content-center
                  .col-lg-8
                    h1.display-4.fs-2.fs-lg-3.fs-xl-4

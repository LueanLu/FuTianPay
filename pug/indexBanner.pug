section#digital-header.py-0.overflow-hidden.IndexBanner
  #carouselExampleIndicators.carousel.slide(data-ride="carousel")
    ol.carousel-indicators
      li.active(data-target="#carouselExampleIndicators", data-slide-to="0")
      li(data-target="#carouselExampleIndicators", data-slide-to="1")
      li(data-target="#carouselExampleIndicators", data-slide-to="2")
    .carousel-inner
      .carousel-item.active
        .bg-holder.digital-header.overlay-1.d-none.d-lg-block(style="background-image:url(assets/img/indexBanner/Banner.jpg);" data-zanim-xs='{"delay":0,"animation":"zoom-out"}' data-zanim-trigger="scroll")
        .bg-holder.digital-header.overlay-1.d-block.d-lg-none(style="background-image:url(assets/img/indexBanner/Banner_s.jpg);" data-zanim-xs='{"delay":0,"animation":"zoom-out"}' data-zanim-trigger="scroll")

        //- Text Content
        .container.IndexBannerContent
          .row.min-vh-100.align-items-center.align-items-lg-end.py-lg-10.pt-10.justify-content-center.text-left
            .col-lg-8.col-xl-7(data-zanim-timeline="{}" data-zanim-trigger="scroll")
              .text-white.overflow-hidden.parallax(data-rellax-speed="3")
                .font-weight-normal(data-zanim-xs='{"delay":1,"duration":1.5}')
                  h3.Title {{ $t('IndexBanner.Title'/*單身動物園*/) }}
              .text-white.parallax(data-rellax-speed="2")
                span.overflow-hidden.d-block(v-if='language !== "en" && language !== "jp"')
                  span.d-inline-block.MainTitle(data-zanim-xs='{"delay":1,"duration":1.5}')
                    h6.MainTitleOne.d-inline {{ $t('IndexBanner.MainTitleOne'/*The Lobster*/) }}
                span.overflow-hidden.d-block
                  h6.MainTitleTwo.d-inline-block.d-block(data-zanim-xs='{"delay":1,"duration":1.5}') {{ $t('IndexBanner.MainTitleTwo'/*黑色幽默愛情片*/) }}
              .text-white.overflow-hidden.parallax(data-rellax-speed="1")
                .font-weight-normal.d-flex.flex-column(data-zanim-xs='{"delay":1,"duration":1.5}')
                  span.Subheading.d-inline-block {{ $t('IndexBanner.SubheadingOne'/*柯林法洛、蕾雅瑟杜、瑞秋懷茲、班維蕭主演*/) }}

        //- Bg Img
        .AnimateBg(data-zanim-timeline='{}' data-zanim-trigger="scroll")
          .BgR(data-rellax-speed="1" data-zanim-xs='{"from":{"opacity":0,"y":150,"scale":1},"to":{"opacity":1,"y":0,"scale":1},"ease":"Back.easeOut","duration":1.5,"delay":2}')
            img(src='assets/img/indexBanner/BgR.svg')
          .BgY(data-rellax-speed="1" data-zanim-xs='{"from":{"opacity":0,"y":-150,"scale":1},"to":{"opacity":1,"y":0,"scale":1},"ease":"Back.easeOut","duration":1.5,"delay":1}')
            img(src='assets/img/indexBanner/BgY.svg')

